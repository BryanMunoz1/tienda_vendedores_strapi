{
  "kind": "collectionType",
  "collectionName": "tiendas",
  "info": {
    "singularName": "tienda",
    "pluralName": "tiendas",
    "displayName": "Tiendas",
    "description": "Colecci√≥n de tiendas con sus vendedores asociados"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "direccion": {
      "type": "string",
      "required": true
    },
    "telefono": {
      "type": "string",
      "required": true
    },
    "correo_electronico": {
      "type": "email",
      "required": true
    },
    "estado": {
      "type": "enumeration",
      "enum": ["activa", "inactiva", "incompleta"],
      "default": "activa"
    },
    "vendedores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vendedore.vendedore",
      "mappedBy": "tienda"
    }
  }
}